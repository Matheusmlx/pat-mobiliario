ermissaoEdicao: {\n                type: Boolean,\n                default: false\n            }\n        },\n        computed: {\n            fieldValue: {\n                get() {\n                    return this.value\n                },\n                set(value) {\n                    this.$emit('changeValue', this.name, value)\n                }\n            },\n            getRefName() {\n                return 'autoComplete'+this.name\n            }\n        },\n        methods: {\n            tratarEventoAlteracao(value) {\n                this.$emit('change', value)\n            },\n            obterLarguraAutoComplete(ref) {\n                if (this.$refs[ref] && this.$refs[ref].$el) {\n                    return {\n                        width: (this.$refs[ref].$el.clientWidth-30)+'px'\n                    }\n                }\n                return {}\n            }\n        }\n    }\n</script>\n\n<style lang=\"stylus\" scoped>\n    .cursor-pointer\n        cursor pointer\n</style>\n"],"sourceRoot":"src/views/components/autocomplete"}]}