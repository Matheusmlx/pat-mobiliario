o: TooltipInformativo\n  },\n  data: function data() {\n    return {\n      paginacaoInterna: this.paginacao,\n      nomeTela: 'INCORPORACAO_ITEM_LISTAGEM_PATRMONIO'\n    };\n  },\n  watch: {\n    paginacaoInterna: {\n      handler: function handler(novoValor) {\n        this.$emit('paginar', novoValor);\n      },\n      deep: true\n    }\n  },\n  computed: {\n    colunas: function colunas() {\n      return this.criarColunas(5, [], ['uriImagem', 'numero', 'valorAquisicao', 'numeroSerie', 'acoes'], [false, true, true, true, false], ['left', 'left', 'left', 'left', 'right'], ['10px', '80px', '120px', '120px', '300px'], 'gray--text');\n    }\n  },\n  methods: {\n    tratarEventoRedirecionarFichaPatrimonio: function tratarEventoRedirecionarFichaPatrimonio(item) {\n      this.$emit('redirecionarFichaPatrimonio', item);\n    }\n  }\n};",{"version":3,"sources":["ModalItensRegistroTabelaEquipamentoArmamento.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmDA,OAAA,kBAAA,MAAA,6CAAA;AACA,OAAA,kBAAA,MAAA,+CAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,8CADA;AAEA,EAAA,KAAA,EAAA,CAAA,OAAA,EAAA,WAAA,EAAA,YAAA,CAFA;AAGA,EAAA,UAAA,EAAA;AAAA,IAAA,kBAAA,EAAA,kBAAA;AAAA,IAAA,kBAAA,EAAA;AAAA,GAHA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,gBAAA,EAAA,KAAA,SADA;AAEA,MAAA,QAAA,EAAA;AAFA,KAAA;AAIA,GATA;AAUA,EAAA,KAAA,EAAA;AACA,IAAA,gBAAA,EAAA;AACA,MAAA,OADA,mBACA,SADA,EACA;AACA,aAAA,KAAA,CAAA,SAAA,EAAA,SAAA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA;AAJA;AADA,GAVA;AAkBA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,aAAA,KAAA,YAAA,CAAA,CAAA,EACA,EADA,EAEA,CAAA,WAAA,EAAA,QAAA,EAAA,gBAAA,EAAA,aAAA,EAAA,OAAA,CAFA,EAGA,CAAA,KAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,KAAA,CAHA,EAIA,CAAA,MAAA,EAAA,MAAA,EAAA,MAAA,EAAA,MAAA,EAAA,OAAA,CAJA,EAKA,CAAA,MAAA,EAAA,MAAA,EAAA,OAAA,EAAA,OAAA,EAAA,OAAA,CALA,EAMA,YANA,CAAA;AAOA;AATA,GAlBA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,uCADA,mDACA,IADA,EACA;AACA,WAAA,KAAA,CAAA,6BAAA,EAAA,IAAA;AACA;AAHA;AA7BA,CAAA","sourcesContent":["<template>\n    <div>\n        <v-layout column>\n            <v-data-table\n                ref=\"table\"\n                :headers=\"colunas\"\n                :items=\"itens\"\n                :server-items-length=\"totalItens\"\n                :options.sync=\"paginacaoInterna\"\n                :loading=\"false\"\n                no-data-text=\"Não há registros de patrimônios\"\n                class=\"tabela-modal-incorporacao-itens\"\n                @click:row=\"tratarEventoRedirecionarFichaPatrimonio\"\n                must-sort\n                hide-default-footer>\n                <template v-slot:header.uriImagem>\n                    <label-personalizado campo=\"uriImagem\" :tela=\"'ITEM_INCORPORACAO'\"/>\n                </template>\n                <template v-slot:header.numero>\n                    <label-personalizado campo=\"numero\" :tela=\"nomeTela\" :apenasNome=\"true\"/>\n                </template>\n                <template v-slot:header.valorAquisicao>\n                    <label-personalizado campo=\"valorAquisicao\" :tela=\"nomeTela\" :apenasNome=\"true\"/>\n                </template>\n                <template v-slot:header.numeroSerie>\n                    <label-personalizado campo=\"numeroSerie\" :tela=\"nomeTela\"/>\n                </template>\n                <template v-slot:item.uriImagem=\"{item}\">\n                    <v-img :src=\"'data:image/png;base64,' + item.imagem\" max-width=\"40\" max-height=\"40\" aspect-ratio=\"1\" class=\"img-patrimonio-registro\"/>\n                </template>\n                <template v-slot:item.numero=\"{item}\">\n                    <span class=\"d-inline-block text-truncate max-10\">{{item.numero}}</span>\n                </template>\n                <template v-slot:item.valorAquisicao=\"{item}\">\n                        <span v-if=\"item.diferencaDizima\" class=\"d-inline-block text-truncate max-13\">\n                            {{ item.valorAquisicao | valorParaDinheiro }}\n                            <tooltip-informativo :tela=\"nomeTela\" campos=\"valorAquisicao\"\n                                                 mensagemPadraoTooltip=\"Esse patrimônio recebeu um arrendondamento devido a dízimas periódicas\"/>\n                        </span>\n                    <span v-else\n                          class=\"d-inline-block text-truncate max-13\">{{ item.valorAquisicao | valorParaDinheiro }}</span>\n                </template>\n                <template v-slot:item.numeroSerie=\"{item}\">\n                    <span class=\"d-inline-block text-truncate max-10\">{{item.numeroSerie | textoSemValor}}</span>\n                </template>\n            </v-data-table>\n        </v-layout>\n    </div>\n</template>\n\n<script>\n    import LabelPersonalizado from '@/views/components/label/LabelPersonalizado'\n    import TooltipInformativo from '@/views/components/tooltip/TooltipInformativo'\n\n    export default {\n        name: 'ModalItensRegistroTabelaEquipamentoArmamento',\n        props: ['itens', 'paginacao', 'totalItens'],\n        components:{LabelPersonalizado, TooltipInformativo},\n        data() {\n            return {\n                paginacaoInterna: this.paginacao,\n                nomeTela: 'INCORPORACAO_ITEM_LISTAGEM_PATRMONIO'\n            }\n        },\n        watch: {\n            paginacaoInterna: {\n                handler(novoValor) {\n                    this.$emit('paginar', novoValor)\n                },\n                deep: true,\n            },\n        },\n        computed:{\n            colunas(){\n                return this.criarColunas(5,\n                                         [],\n                                         ['uriImagem','numero','valorAquisicao','numeroSerie','acoes'],\n                                         [false,true,true,true,false],\n                                         ['left','left','left','left','right'],\n                                         ['10px','80px','120px','120px','300px'],\n                                         'gray--text')\n            }\n        },\n        methods: {\n            tratarEventoRedirecionarFichaPatrimonio(item) {\n                this.$emit('redirecionarFichaPatrimonio', item)\n            }\n        }\n    }\n</script>\n\n<style lang=\"stylus\">\n    .img-patrimonio-registro\n        border 1px solid #777\n</style>\n"],"sourceRoot":"src/views/components/modal/incorporacao/registroIncorporacao"}]}