              <span class=\"d-inline-block text-truncate max-10\">\n                        {{ item.quantidadeReservada | formataValor }}\n                        <tooltip-informativo\n                            icon=\"info\"\n                            color=\"orange darken-3\"\n                            class=\"tooltipRestante\"\n                            :nudge-left=\"35\"\n                            :mensagemPadraoTooltip=\"formataMensagemRestantesTooltip(item.quantidadeRestante)\"\n                            v-if=\"existeRestante(item.quantidadeRestante)\"\n                        />\n                    </span>\n                </template>\n                <template v-slot:item.orgaos=\"{item}\">\n                    <span class=\"d-inline-block text-truncate max-20\" v-if=\"item.orgaos && item.orgaos.length\">\n                        <v-tooltip top max-width=\"400\">\n                            <template v-slot:activator=\"{ on }\">\n                                <div v-on=\"on\">\n                                    <span>{{ formataOrgaosParaExibicao(item.orgaos) }}</span>\n                                </div>\n                            </template>\n                            {{ item.orgaos.join(', ') }}\n                        </v-tooltip>\n                    </span>\n                    <span v-else>{{ '' | textoSemValor }}</span>\n                </template>\n                <template v-slot:item.situacao=\"{item}\">\n                    <span class=\"d-inline-block text-truncate max-15\">\n                        <v-icon class=\"mr-1\" color=\"orange\" size=\"13\" v-if=\"situacaoParcialOuElaboracao(item)\">\n                            fas fa-edit\n                        </v-icon>\n                        {{ item.situacao | azEnum(situacoesReservas) }}\n                    </span>\n                </template>\n                <template v-slot:item.acoes=\"{  }\">\n                    <v-icon>keyboard_arrow_right</v-icon>\n                    <span class=\"mobile\">Acessar</span>\n                </template>\n            </v-data-table>\n            <paginacao :paginacao-interna=\"paginacaoInterna\" :paginas=\"paginas\" @resetaPage=\"resetaPage\"/>\n        </az-form>\n    </az-container>\n</template>\n\n<script>\n    import {mapMutations} from 'vuex'\n    import {mutationTypes} from '@/core/constants'\n    import Paginacao from '@/views/components/tabela/Paginacao'\n    import TooltipInformativo from '@/views/components/tooltip/TooltipInformativo'\n\n    export default {\n        name: 'ReservaListagemTabela',\n        components: {Paginacao, TooltipInformativo},\n        props: ['itens', 'paginacao', 'paginas', 'totalItens'],\n        data() {\n            return {\n                paginacaoInterna: this.paginacao\n            }\n        },\n        computed: {\n            colunas() {\n                return this.criarColunas(\n                    6,\n                    ['Código', 'Criação', 'Quantidade', 'Órgão(s)', 'Situação', ''],\n                    ['codigo', 'dataCriacao', 'quantidadeReservada', 'orgaos', 'situacao', 'acoes'],\n                    [false, false, false, false, false, false],\n                    ['left', 'left', 'left', 'left', 'left', 'right'],\n                    ['13%', '13%', '15%', '25%', '27%', '5%'],\n                    'gray--text')\n            }\n        },\n        methods: {\n            ...mapMutations([mutationTypes.CONFIGURACAO.RESERVA.RESETA_PAGE_RESERVA]),\n            tratarEventoAcessar(item) {\n                this.$emit('acessar', item)\n            },\n            tratarPaginacao(pagina) {\n                this.paginacaoInterna.page = pagina\n            },\n            resetaPage() {\n                this.resetaPageReserva()\n            },\n            situacaoParcialOuElaboracao(item) {\n                return (['PARCIAL', 'EM_ELABORACAO'].includes(item.situacao))\n            },\n            formataOrgaosParaExibicao(orgaos) {\n                if (orgaos.length > 2) {\n                    return `${orgaos[0]}, ${orgaos[1]}... +${orgaos.length - 2}`\n                } else {\n                    return orgaos.join(', ')\n                }\n            },\n            existeRestante(restante) {\n                return restante > 0\n            },\n            formataMensagemRestantesTooltip(restante) {\n                const restanteFormatado = this.$options.filters.azNumber(restante)\n                return `${restanteFormatado} Restantes`\n            }\n        },\n        watch: {\n            paginacaoInterna: {\n                handler(novoValor) {\n                    this.$emit('paginar', novoValor)\n                },\n                deep: true,\n            },\n        },\n    }\n</script>\n\n<style lang=\"stylus\">\n    .tooltipRestante .iconeTooltipInformativo\n        margin-left 3px !important\n        margin-bottom 0px !important\n        margin-top 2px !important\n</style>\n"],"sourceRoot":"src/views/pages/configuracao/reserva/global/listagem"}]}