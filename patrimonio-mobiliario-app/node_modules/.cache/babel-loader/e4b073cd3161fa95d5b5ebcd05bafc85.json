       this.$emit('paginar', novoValor)\n                },\n                deep: true,\n            },\n        },\n    }\n</script>\n\n<style lang=\"stylus\">\n        .comboLicenciamento\n            padding-top 0\n            .v-select__selection\n                color #858585!important\n                font-size 13px\n                margin-bottom 0\n            .v-input__icon .v-icon\n                font-size 22px\n                margin-bottom 0\n\n</style>\n"],"sourceRoot":"src/views/components/modal/incorporacao/adicionaritem/itempatrimonio/edicao/tipoVeiculo"}]}