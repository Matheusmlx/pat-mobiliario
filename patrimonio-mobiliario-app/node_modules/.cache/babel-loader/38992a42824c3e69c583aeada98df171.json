n        <v-btn class=\"az-pdf-toolbar__content\" @click=\"$emit('zoomIn')\" icon data-test=\"zoomIn\" :disabled=\"disableButtons\">\n            <v-tooltip bottom open-delay=\"800\">\n                <span>Zoom +</span>\n                <v-icon slot=\"activator\" >zoom_in</v-icon>\n            </v-tooltip>\n        </v-btn>\n        <div class=\"az-pdf-toolbar__pagination\" data-test=\"pagination\">\n            {{ pagination.current || '-' }} / {{ pagination.total || '-' }}\n        </div>\n        <v-spacer />\n        <v-btn \n            class=\"az-pdf-toolbar__content\"\n            @click=\"$emit('rotate')\"\n            v-if=\"rotateButton\"\n            icon\n            data-test=\"rotate\"\n            :disabled=\"disableButtons\"\n        >\n            <v-tooltip bottom open-delay=\"800\">\n                <span>Girar</span>\n                <v-icon slot=\"activator\">rotate_right</v-icon>\n            </v-tooltip>\n        </v-btn>\n        <v-btn\n            class=\"az-pdf-toolbar__content\"\n            @click=\"$emit('download')\"\n            v-if=\"downloadButton\"\n            icon\n            data-test=\"download\"\n            :disabled=\"disableButtons\"\n        >\n            <v-tooltip bottom open-delay=\"800\">\n                <span>Download</span>\n                <v-icon slot=\"activator\" >get_app</v-icon>\n            </v-tooltip>\n        </v-btn>\n        <v-btn\n            class=\"az-pdf-toolbar__content\"\n            @click=\"$emit('print')\"\n            v-if=\"printButton\"\n            icon\n            data-test=\"print\"\n            :disabled=\"disableButtons\"\n        >\n            <v-tooltip bottom open-delay=\"800\">\n                <span>Imprimir</span>\n                <v-icon slot=\"activator\">print</v-icon>\n            </v-tooltip>\n        </v-btn>\n    </v-toolbar>\n</template>\n\n<script>\nexport default {\n    name: 'Toolbar',\n    props: {\n        disableButtons: {\n            type: Boolean,\n         